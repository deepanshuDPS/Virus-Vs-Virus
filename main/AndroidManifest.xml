<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Game.Rowdy.virusvsvirus">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/vvv2"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <activity
            android:name=".Start"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TwoPlayer"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ThreePlayer"
            android:screenOrientation="portrait" />
        <activity
            android:name=".FourPlayer"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SetPlayer"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SelPlayer"
            android:screenOrientation="portrait" />
        <activity android:name=".Instruction"
            android:screenOrientation="portrait"/>
    </application>

</manifest>